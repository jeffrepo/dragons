<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="dragons_res_users_form_inherit" model="ir.ui.view">
            <field name="name">dragons.res.users.form.inherit</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <group name="messaging" position="after">
                    <group name="Others">
                        <field name="warehouse_ids" widget="many2many_tags" options="{'no_create': True}"/>
                        <field name="location_ids" widget="many2many_tags" options="{'no_create': True}"/>
                    </group>
                </group>
            </field>
        </record>
    </data>
</odoo>